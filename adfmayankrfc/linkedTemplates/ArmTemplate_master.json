{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfmayankrfc"
		},
		"AdventureWorks_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AdventureWorks'"
		},
		"Metastore_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Metastore'"
		},
		"SupportDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SupportDatabase'"
		},
		"ConfigDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ConfigDB'"
		},
		"src_AdventureWorks_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'src_AdventureWorks'"
		},
		"Landing_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Landing'"
		},
		"Landing_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sandboxadlsmayank.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfmayankrfc_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdventureWorks_connectionString": {
						"value": "[parameters('AdventureWorks_connectionString')]"
					},
					"Metastore_connectionString": {
						"value": "[parameters('Metastore_connectionString')]"
					},
					"SupportDatabase_connectionString": {
						"value": "[parameters('SupportDatabase_connectionString')]"
					},
					"ConfigDB_connectionString": {
						"value": "[parameters('ConfigDB_connectionString')]"
					},
					"src_AdventureWorks_connectionString": {
						"value": "[parameters('src_AdventureWorks_connectionString')]"
					},
					"Landing_accountKey": {
						"value": "[parameters('Landing_accountKey')]"
					},
					"Landing_properties_typeProperties_url": {
						"value": "[parameters('Landing_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}